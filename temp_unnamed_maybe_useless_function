# cat match output where messy_df is large list and clean df is list that MIGHT be in the messy
cat_match(Survey_Ids$Utility_Name__c,df2$UtilityName,method = "lcs") -> t

# loop takes the output and finds the closest match, if the 1 on line 8 is changed it returns the x closest matches
min_dists = data.frame()
for (name in unique(Survey_Ids$Utility_Name__c)){
  filter(t, match == name) %>% arrange(dists) -> min_dist
  min_dist <- min_dist[1,]
  min_dists <- rbind(min_dists,min_dist)
}
